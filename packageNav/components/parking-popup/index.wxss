/* packageNav/components/parking-popup/index.wxss */
.mask{
  position: absolute;
  width: 100%;
  height: 100%;
  left: 0;
  top: 0;
  z-index: 99;
}
.parking_popup{
  position: absolute;
  width: 100%;
  max-height: 1080rpx;
  padding-bottom: 30rpx;
  box-sizing: border-box;
  left: 0;
  bottom: 0;
  z-index: 100;
  color: #313131;
}
.popup_plate_height{
  height: 1080rpx;
  display: flex;
  justify-content: space-between;
  flex-direction: column;
}
.parking_main{
  width: 718rpx;
  padding: 30rpx;
  margin: 0 auto;
  background: #fff;
  box-shadow: 0rpx 2rpx 6rpx 0rpx rgba(134,109,67,0.62);
  border-radius: 8px;
  border: 1rpx solid #FF7800;
  box-sizing: border-box;
  overflow: hidden;
  transition: height .4s;
}
.plate_height{
  height: 530rpx;
}
.plate_height_two{
  height: 450rpx;
}
.stall_height{
  height: 290rpx;
}
.parking_main_unfold{
  height: 1050rpx;
}
.parking_name{
  display: flex;
  align-items: center;
  font-size: 34rpx;
  font-weight: 600;
  color: #313131;
  line-height: 1;
}
.parking_name_icon{
  width: 32rpx;
  height: 36rpx;
  margin-right: 12rpx;
}
.parking_tab{
  display: flex;
  justify-content: space-between;
  margin-top: 30rpx;
}
.parking_tab_item{
  width: 316rpx;
  height: 50rpx;
  border-radius: 4px;
  font-size: 24rpx;
  font-weight: 600;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content: center;
  border: 1rpx solid #FF7800;
  color: #FF7800;
}
.parking_active_tab{
  background-color: #FF7800;
  color: #fff;
}
.plate_number{
  font-size: 36rpx;
  font-weight: 600;
  color: #FF7800;
  text-align: center;
  margin: 40rpx auto 30rpx;
}
.plate_number_input{
  height: 128rpx;
  border-radius: 8px;
  border: 1px solid #FF7800;
  box-sizing: border-box;
  padding: 0 16rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 36rpx;
  font-weight: 600;
  margin-bottom: 100rpx;
  color: #FF7800;
}
.num_province{
  width: 64rpx;
  height: 102rpx;
  color: #fff;
  background-color: #FF7800;
  border-radius: 5px 0rpx 0rpx 5px;
  border: 1px solid #FF7800;
  display: flex;
  align-items: center;
  justify-content: center;
  box-sizing: border-box;
}
.num_code{
  width: 64rpx;
  height: 102rpx;
  border-radius: 0px 5px 5px 0px;
  border: 1px solid #FF7800;
  display: flex;
  align-items: center;
  justify-content: center;
  box-sizing: border-box;
}
.num_detail{
  width: 64rpx;
  height: 102rpx;
  border-bottom: 1px solid #FF7800;
  display: flex;
  align-items: center;
  justify-content: center;
  box-sizing: border-box;
}
.num_plus{
  position: relative;
  width: 64rpx;
  height: 102rpx;
  border-radius: 5px;
  border: 1px solid #FF7800;
  display: flex;
  align-items: center;
  justify-content: center;
  box-sizing: border-box;
  font-size: 8px;
  font-weight: 500;
  color: #FF7800;
}
.num_plus_one{
  font-size: 48rpx;
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%, calc(-50% - 10rpx));
}
.num_plus_two{
  position: absolute;
  width: 100%;
  left: 0;
  bottom: 6rpx;
  text-align: center;
}
.plate_result{
  text-align: center;
  padding-top: 38rpx;
}
.plate_result_label{
  font-size: 40rpx;
  font-weight: 600;
  margin-bottom: 32rpx;
}
.plate_result_val{
  font-size: 34rpx;
  font-weight: 600;
  margin-bottom: 114rpx;
}
.plate_start{
  width: 380rpx;
  height: 108rpx;
  margin: 0 auto;
  background-color: #FF7800;
  color: #fff;
  border-radius: 8px;
  line-height: 108rpx;
  font-size: 38rpx;
  font-weight: 600;
  letter-spacing: 1px;
}
.plate_result_no{
  padding-top: 84rpx;
  font-size: 34rpx;
  font-weight: 600;
  line-height: 64rpx;
  text-align: center;
}
.stall_input_label{
  height: 50rpx;
  margin-top: 30rpx;
  font-size: 24rpx;
  font-weight: 500;
  display: flex;
  align-items: flex-end;
}
.stall_input_content{
  padding: 40rpx 0 52rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.input_view{
  width: 648rpx;
  height: 64rpx;
  background-color: #fff;
  border-radius: 12px;
  display: flex;
  align-items: center;
  /* justify-content: space-between; */
  box-sizing: border-box;
  border: 1px solid #eee;
  padding: 0 22rpx;
}
.input_view_search{
  width: 584rpx;
}
.search_icon{
  width: 32rpx;
  height: 32rpx;
}
.stall_input{
  width: calc(100% - 100rpx);
  height: 100%;
  font-size: 24rpx;
  font-weight: 500;
  margin: 0 20rpx;
}
.search_close_icon{
  /* position: absolute; */
  width: 32rpx;
  height: 32rpx;
  /* top: 50%;
  transform: translateY(-50%);
  right: 32rpx; */
}
.search_cancel{
  font-size: 24rpx;
  font-weight: 500;
  color: #313131;
}
.stall_building{
  height: 48rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  color: #313131;
  margin-bottom: 40rpx;
}
.building_label{
  font-size: 26rpx;
  font-weight: 600;
  line-height: 1;
}
.building_list{
  font-size: 24rpx;
  width: 570rpx;
  font-weight: 500;
  display: flex;
  flex-wrap: nowrap;
  overflow-x: scroll;
}
.building_list::-webkit-scrollbar{
  display:none
}
.building_item{
  height: 100%;
  line-height: 48rpx;
  padding: 0 24rpx;
  border-radius: 8px;
  margin-right: 46rpx;
  box-sizing: border-box;
  border: 1px solid #FF7800;
  color: #FF7800;
  flex-shrink: 0;
}
.building_select{
  background-color: #FF7800;
  color: #fff;
}
.stall_list_title{
  padding: 0 30rpx;
  font-size: 34rpx;
  font-weight: 600;
}
.stall_list{
  padding: 44rpx 0 14rpx;
}
.stall_list_filter{
  height: 480rpx;
  box-sizing: border-box;
  overflow-y: scroll;
  padding: 14rpx 0;
}
.stall_item{
  padding: 0 30rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-bottom: 30rpx;
}
.stall_num{
  font-size: 34rpx;
  font-weight: 600;
  color: #313131;
  line-height: 1;
}
.stall_btn{
  width: 316rpx;
  height: 50rpx;
  border-radius: 4px;
  font-size: 24rpx;
  font-weight: 600;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content: center;
  background-color: #FF7800;
  color: #fff;
}
.stall_search_list{
  height: 730rpx;
  overflow-y: scroll;
}
.keyboard_province{
  width: 750rpx;
  /* height: 436rpx; */
  margin: 100rpx 0 -30rpx;
  background-color: #D1D4DB;
  box-sizing: border-box;
  padding: 20rpx 10rpx;
}
.province_view{
  position: relative;
  display: flex;
  justify-content: center;
  margin-bottom: 8rpx;
}
.keyboard_item{
  width: 66rpx;
  height: 90rpx;
  background-color: #fff;
  border-radius: 3px;
  font-size: 36rpx;
  font-weight: 600;
  color: #000;
  display: flex;
  align-items: center;
  justify-content: center;
}
.province_view .keyboard_item{
  margin-right: calc((100% - 66rpx * 10) / 9);
}
.province_view .keyboard_item:last-child{
  margin-right: 0;
}
.keyboard_del{
  position: absolute;
  bottom: 0;
  right: 0;
  width: 140rpx;
  height: 90rpx;
  background-color: #fff;
  border-radius: 3px;
  display: flex;
  align-items: center;
  justify-content: center;
}
.keyboard_del image{
  width: 52rpx;
  height: 52rpx;
}
.keyboard_num{
  position: relative;
  width: 750rpx;
  /* height: 436rpx; */
  margin: 100rpx 0 -30rpx;
  background-color: #D1D4DB;
  box-sizing: border-box;
  padding: 20rpx 10rpx 12rpx;
  display: flex;
  flex-wrap: wrap;
}
.keyboard_num .keyboard_item{
  margin-right: calc((100% - 66rpx * 10) / 9);
  margin-bottom: 8rpx;
}
.keyboard_num .keyboard_item:nth-child(10n + 10){
  margin-right: 0;
}
.keyboard_num .keyboard_del{
  bottom: 20rpx;
  right: 10rpx;
}