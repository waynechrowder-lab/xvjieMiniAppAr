<poster wx:if="{{isSave}}" width="{{posterWidth}}" height="{{posterHeight}}" pixelRatio="{{pixelRatio}}" sourceParams="{{posterSources}}"
  save="{{save}}" bind:start="onPosterStart" bind:finish="onPosterFinish" bind:fail="onPosterFail" bind:saveTemp="onPosterSaveTemp" bind:saved="onPosterSaved" bind:saveError="onPosterSaveError"
></poster>
