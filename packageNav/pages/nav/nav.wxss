/* plugin/pages/nav/nav.wxss */
page{
  height: 100%;
  overflow: hidden;
  background-color: #000;
}
.page{
  width: 100%;
  height: 100%;
}
.mask{
  position: absolute;
  width: 100%;
  height: 100%;
  left: 0;
  top: 0;
  z-index: 99;
}
.takePhoto_click{
  width: 200rpx;
  height: 80rpx;
  position: absolute;
  left: 50rpx;
  top: 50rpx;
  border-radius: 20rpx;
  z-index: 1000;

  background: #ffecad;
  font-size: 34rpx;
  font-weight: 500;
  color:#552625;
  display: flex;          /* 开启Flex布局 */
  justify-content: center;/* 水平居中 */
  align-items: center;    /* 垂直居中 */
}

.checkinMask {
  position: fixed;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  display: flex;
  align-items: center;
  justify-content: center;
  background: rgba(0, 0, 0, 0.6);
  z-index: 9999;
  /* flex-direction: column; */
  padding: 10rpx;
}

.checkinPanel{
  width: 80vw;
  max-height: 110vh;
  min-height: 80vh; 
  box-sizing: border-box;
  display: flex;
  flex-direction: column;

  padding: 40rpx 40rpx 20rpx;

  position: relative;   /* ✅ 必须：建立定位上下文 */
  background: transparent;
  align-items: center;
}

.checkinPanel::after{
  content: "";
  position: absolute;
  left: 0; top: 0;
  width: 100%;
  height: 100%;

  background-image: url('https://shanghai-mashu-wxapp.oss-cn-shanghai.aliyuncs.com/ui/zhengding/nav/photo-preview.png');
  background-repeat: no-repeat;
  background-size: 100% 100%;

  z-index: 20;          /* ✅ 比截图大 */
  pointer-events: none; /* ✅ 不挡按钮点击 */

  transform: translateY(-10%);
  background-position: center top;
}

/* ✅ 截图：宽度占满“内窗”，高度自适应 */
.checkinShot{
  width: 90%;
  height: auto;
  display: block;
  margin: 0 auto;

  position: relative;   /* ✅ 让 z-index 生效 */
  z-index: 10;
  transform: translateY(-15%);
}

.checkinSave{
  position: absolute;
  bottom: 220rpx;
  width: 320rpx;
  height: 95rpx;
  line-height: 84rpx;
  text-align: center;

  /* border-radius: 999rpx; */
  /* background: rgba(0,0,0,0.65);
  color: #fff; */
  z-index: 30;
  background-image: url('https://shanghai-mashu-wxapp.oss-cn-shanghai.aliyuncs.com/ui/zhengding/nav/photo_save.png');
  background-size: contain;
}

.checkinClose{
  position: absolute;
  bottom: 20rpx;
  margin-top: 0rpx;          /* ✅ 与面板间距 */
  width: 72rpx;
  height: 72rpx;
  line-height: 84rpx;
  text-align: center;
  /* border-radius: 999rpx; */
  /* background: rgba(0,0,0,0.65);
  color: #fff; */

  z-index: 50;

  background-image: url('https://shanghai-mashu-wxapp.oss-cn-shanghai.aliyuncs.com/ui/zhengding/nav/photo_close.png');
  background-size: contain;
}

