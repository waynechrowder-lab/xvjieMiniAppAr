  width: 686rpx;
  height: 120rpx;
  left: 50%;
  transform: translateX(-50%);
  bottom: 50rpx;
  /* border-radius: 16rpx; */
  /* background-color: #fff; */
  z-index: 120;
  background-image: url("https://shanghai-mashu-wxapp.oss-cn-shanghai.aliyuncs.com/ui/zhengding/nav/nav-running.png");
  background-size: contain;
}
.popup_exit{
  position: absolute;
  width: 686rpx;
  height: 120rpx;
  left: 50%;
  transform: translateX(-50%);
  bottom: 50rpx;
  /* border-radius: 16rpx; */
  /* background-color: #fff; */
  z-index: 120;
  background-image: url("https://shanghai-mashu-wxapp.oss-cn-shanghai.aliyuncs.com/ui/zhengding/nav/nav-exit.png");
  background-size: contain;
}

.nav_info{
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
}
.nav_name{
  width: 140rpx;
  margin-left: 20rpx;
  text-align: center;
  color: #FFE3AE;
  font-size: 32rpx;
  line-height: 1;
  font-weight: 600;
}
.nav_detail{
  width: 340rpx;
  height: 80rpx;
  margin-left: 75rpx;
  font-size: 26rpx;
  font-weight: 400;
  color: #FFE3AE;
  display: flex;
  align-items: center;
  justify-content: center;
  box-sizing: border-box;
  /* border-left: 2rpx solid #FFE3AE;
  border-right: 2rpx solid #FFE3AE; */
}
.arrive_txt{
  font-weight: 400;
}
.nav_close_icon{
  opacity: 0;
  width: 160rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.nav_close_icon image{
  width: 32rpx;
  height: 32rpx;
}
.nav_exit{
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
}
.exit_title, .exit_cancel{
  width: 50%;
  height: 80rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  box-sizing: border-box;
}
.exit_title{
  border-right: 2rpx solid #FFE3AE;
  font-size: 36rpx;
  font-weight: 600;
  color: #ffc3b8;
}
.exit_cancel{
  font-size: 36rpx;
  font-weight: 600;
  color: #FFE3AE;
}
.current_floor{
  position: absolute;
  width: 98rpx;
  height: 98rpx;
  background-color: rgba(0, 0, 0, 0.7);
  border-radius: 16rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 28rpx;
  font-weight: 600;
  color: #fff;
  left: 32rpx;
  top: 32rpx;
  z-index: 100;
}
.auth_layer{
  position: absolute;
  width: 100%;
  height: 100%;
  left: 0;
  top: 0;
  background-color: rgba(0, 0, 0, 0.7);
  z-index: 140;
}
.auth_content{
  position: absolute;
  width: 560rpx;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
  background-color: #fff;
  border-radius: 16rpx;
  padding: 70rpx 48rpx 48rpx;
  box-sizing: border-box;
}
.auth_txt{
  font-size: 32rpx;
  font-weight: 400;
  color: #141A27;
  text-align: center;
  margin-bottom: 70rpx;
}
.auth_btn_group{
  display: flex;
  justify-content: space-between;
}
.auth_exit_btn, .auth_setting_btn{
  width: 216rpx;
  height: 96rpx;
  border-radius: 16rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 32rpx;
}
.auth_exit_btn{
  background-color: #F0F0F3;
  color: #444253;
  font-weight: 400;
}
.auth_setting_btn{
  color: #fff;
  background: linear-gradient( 132deg, #3C73E7 0%, #0E1991 100%);
  font-weight: 600;
}
.search_main{
  width: 100%;
  height: 790rpx;
  margin-top: 36rpx;
  overflow-y: scroll;
  box-sizing: border-box;
  padding: 0 32rpx;
}
.search_res_content{
  padding: 16rpx 0;
  border-bottom: 2rpx solid rgba(240, 240, 243, 0.5);
}
.search_item{
  border: none;
}
.search_item .shop_info{
  width: 478rpx;
  margin-left: 50rpx;
}
.search_item .shop_title{
  margin-bottom: 0;
}
.search_res_title{
  font-size: 24rpx;
  font-weight: 400;
  color: rgba(68, 66, 83, 0.5);
  margin-bottom: 16rpx;
}
.floor_judge{
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
}
.floor_judge_title{
  text-align: center;
  font-size: 48rpx;
  font-weight: 600;
  color: #FFFFFF;
  margin-bottom: 90rpx;
}
.floor_judge_floor{
  text-align: center;
  font-size: 144rpx;
  font-weight: bold;
  color: #FFFFFF;
  margin-bottom: 100rpx;
}
.floor_judge_btn{
  width: 480rpx;
  height: 96rpx;
  background-color: #fff;
  border-radius: 16rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 32rpx;
  font-weight: 600;
  color: #141A27;
  margin: 0 auto 32rpx;
}
.popup_class{
  position: absolute;
  width: 654rpx;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
  border-radius: 8px;
  background-color: #fff;
  box-sizing: border-box;
  padding: 28rpx 20rpx 48rpx;
}
.share_title{
  font-size: 32rpx;
  font-weight: 600;
  color: #141A27;
}
.share_content{
  margin: 34rpx 0 48rpx;
  text-align: center;
}
.shop_share1{
  font-size: 26rpx;
  font-weight: 600;
  color: #141A27;
  line-height: 36rpx;
  margin-bottom: 10rpx;
}
.shop_share2{
  font-size: 20rpx;
}
.shop_name_blue{
  color: #4A78DD;
}
.share_btn_group{
  display: flex;
  justify-content: space-around;
  line-height: 0;
}
.cancel_btn{
  width: 216rpx;
  height: 56rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 30rpx;
  font-weight: 400;
  color: #444253;
  background-color: #F0F0F3;
  border-radius: 8px;
}
.cancel_btn2{
  width: 284rpx;
}
.share_btn{
  position: relative;
  width: 216rpx;
  height: 56rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 30rpx;
  font-weight: 600;
  color: #fff;
  background: linear-gradient( 132deg, #3C73E7 0%, #0E1991 100%);
  border-radius: 8px;
}
.share_popup_close{
  position: absolute;
  width: 44rpx;
  height: 44rpx;
  top: 8rpx;
  right: 14rpx;
}
.share_entrance{
  position: absolute;
  width: 136rpx;
  height: 136rpx;
  top: 36rpx;
  right: 16rpx;
  z-index: 99;
}
.feedback_entrance{
  position: absolute;
  width: 136rpx;
  height: 136rpx;
  top: 190rpx;
  right: 16rpx;
  z-index: 99;
}
